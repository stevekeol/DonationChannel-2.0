<cu-custom bgColor="bg-red" isBack="{{true}}">
  <view slot="content">选择求助关键词</view>
  <view slot="backText">返回</view>  
</cu-custom>
<view class="body">
  <view class="title">快速选择求助关键词</view>
  <view class="keyContainer">
    <view class="item {{resKey[index] ? 'choosed' : 'normal'}}" wx:for="{{ keys }}" bindtap="chooseKey" data-index='{{ index }}'>{{ keys[index] }}</view>
  </view>
  <view class="button-lg" bindtap="saveHelpKey">保存</view>
</view>
