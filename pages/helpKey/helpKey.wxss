@import "../../colorui/main.wxss";
.body {
    width: 90%;
    margin-left: 5%;
    padding-top: 30rpx;
}
.keyContainer {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-gap: 20rpx 20rpx;
}
.keyContainer .item {
    height: 120rpx;
    line-height: 120rpx;
    border-radius: 12rpx;
    text-align: center;
}
.keyContainer .normal {
    background-color: grey;
    color: white;
}
.keyContainer .choosed {
    background-color: #e54d42;
    color: white;
}
.title {
    color: grey;
    margin-bottom: 20rpx;
}
.button-lg {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #e54d42;
    border-radius: 50rpx;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    text-align: center;
    color: white;
    font-size: 32rpx;
}